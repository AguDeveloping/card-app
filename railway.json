{
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "docker/railway.Dockerfile",
    "buildContext": "."
  },
  "deploy": {
    "startCommand": "node dist/server.js",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 300,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 3
  }
}