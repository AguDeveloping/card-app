{
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "docker/railway.Dockerfile",
    "buildContext": "."
  },
  "deploy": {
    "startCommand": "node dist/server.js",
    "healthcheckPath": "/",
    "healthcheckTimeout": 100,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}